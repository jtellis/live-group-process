{
  "name": "live-group-process",
  "version": "1.0.0",
  "repository": "git@github.com:jtellis/live-group-process.git",
  "author": "jtellis <jessetellis@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build-server": "yarn webpack --config server.webpack.config.babel.js",
    "build-client": "yarn webpack --config client.webpack.config.babel.js",
    "dev": "yarn clean && yarn concurrently \"yarn build-client --mode development --watch\" \"yarn build-server --mode development && yarn nodemon dist/index.js\" \"yarn build-server --mode development --watch\""
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "nodemon": "^2.0.6",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.2.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongodb": "^3.6.3",
    "morgan": "^1.10.0",
    "node-cleanup": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ws": "^7.4.2",
    "xstate": "^4.15.3"
  }
}
